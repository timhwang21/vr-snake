{
  "name": "vr_test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "devtools": "react-devtools",
    "test": "jest",
    "prettier": "prettier --single-quote --trailing-comma all --no-config --write \"src/**/*.{js,css}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "ovrui": "~2.0.0",
    "react": "16.0.0",
    "react-native": "~0.48.0",
    "react-vr": "~2.0.0",
    "react-vr-web": "~2.0.0",
    "three": "^0.87.0"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "husky": "^0.14.3",
    "jest": "^19.0.2",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "prop-types": "^15.6.0",
    "react-devtools": "^2.5.2",
    "react-test-renderer": "16.0.0",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  },
  "lint-staged": {
    "*.{js,css}": [
      "yarn prettier",
      "git add"
    ]
  }
}
